{
  "name": "tharwa-web",
  "version": "0.0.0",
  "description": "Online mobile Banking application (front-end app)",
  "author": "Tharwa Team <contact@tharwa.com>",
  "main": "src/index.js",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=7.6",
    "yarn": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emGiin/tharwa-web"
  },
  "scripts": {
    "precommit": "yarn lint",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "esw **/*.js --ignore-path .gitignore server config --color",
    "lint:watch": "yarn lint -- --watch",
    "lint:fix": "yarn lint -- --fix",
    "validate": "yarn lint && yarn test",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "^3.2.2",
    "axios": "^0.17.1",
    "mdbreact": "^4.1.0",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.13",
    "@storybook/addon-links": "^3.3.13",
    "@storybook/addons": "^3.3.13",
    "@storybook/cli": "^3.3.13",
    "@storybook/react": "^3.3.13",
    "babel-core": "^6.26.0",
    "eslint": "^4.17.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.43.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "eslint-watch": "^3.1.3",
    "husky": "^0.14.3",
    "react-styleguidist": "^6.2.3"
  }
}
